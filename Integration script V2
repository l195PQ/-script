local userScripts = {
    ["xiuluoshasheng"] = "https://raw.githubusercontent.com/xiuluoshashen/-script/main/%E8%84%9A%E6%9C%ACHUB%E6%9C%80%E6%96%B0%E7%89%88%E6%9C%AC",
    ["User2"] = "https://www.example.com/user2_script.lua",
    ["User3"] = "https://www.example.com/user3_script.lua"
}

local player = game.Players.LocalPlayer
local scriptUrl = userScripts[player.Name]

if not scriptUrl then
    warn("你没有执行此脚本的权限。")
    return
end

-- 加载对应的脚本
loadstring(game:HttpGet(scriptUrl))()
