				if v:IsA("Beam") then
					v.Color = ColorSequence.new{
						ColorSequenceKeypoint.new(0, Color3.new(1, 1, 0.494118)),
						ColorSequenceKeypoint.new(1, Color3.new(1, 1, 0.494118))
					}
					v.Enabled = false
